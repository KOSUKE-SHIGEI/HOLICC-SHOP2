.newsletter-wrapper {
  .input-group__field {
    opacity: 0;

    .supports-no-js &,
    .animations--disabled & {
      opacity: 1;
    }
  }
}

.input-group__btn--newsletter {
  vertical-align: middle;
  opacity: 0;

  .supports-no-js &,
  .animations--disabled & {
    opacity: 1;
  }
}

.btn--newsletter__submit {
  position: relative;
  padding: 2px 7px 0;
  font-size: rem(10);
  font-weight: $font-weight-bold;
  text-transform: uppercase;
  letter-spacing: 0.125em;
  float: right;
  border: none;
  background-color: transparent;

  .icon-arrow-right {
    height: rem(7);
    padding-left: rem(15);
    position: relative;
    top: 2px;
    @include transform(translateY(-50%));
    transition: $card-transition;

    @include media-query($small) {
      top: 7px;
    }
  }
}

@include media-query($medium-up) {
  .btn--newsletter__submit:hover > .icon {
    @include transform(translate(7px, -50%));
  }
}

.newsletter__message {
  padding-top: 20px;
}

/*================ Pagination ================*/
.pagination {
  @include display-flexbox;
  align-items: center;
  justify-content: space-between;
  margin: $gutter 0 ($gutter / 2);
  text-align: center;

  @include media-query($medium-up) {
    max-width: 280px;
    margin: ($gutter * 4) auto 0;
  }
}

.pagination__btn {
  @include display-flexbox;
  @include justify-content(center);
  @include align-items(center);
  width: $gutter * 2;
  height: $gutter * 2;
  fill: $color-primary;
  padding: 0;

  .icon {
    width: 4px;
    height: 7px;
  }
}

.pagination__page {
  padding-right: $gutter;
  font-weight: $font-weight-bold;
  font-size: rem(12);
  letter-spacing: 1.5px;
  color: $color-primary;
  text-decoration: none;

  &:last-child {
    padding-right: 0;
  }

  &:hover,
  &:focus {
    color: currentColor;
  }
}

.pagination__page--current {
  color: $color-body-text;
}

.password-html {
  background-color: $color-main-bg;
}

.password-header {
  margin-bottom: $gutter * 3;
  padding: ($gutter / 2) 0;
  background-color: $color-header;

  @include media-query($medium-up) {
    margin-bottom: $gutter * 5;
  }
}

.password-header__shop-name {
  margin: 0;
}

.password-signup,
.password-login {
  max-width: 460px;
  margin: 0 auto;
}

.password-sub-heading {
  margin-bottom: $gutter;
  font-size: rem(20);

  @include media-query($medium-up) {
    font-size: rem(22);
  }
}

.password-newsletter-subheading {
  margin-bottom: rem(35);
}

.password-signup {
  margin: ($gutter * 2) auto;
  padding: 0 $gutter;
}

.password-sub-action {
  margin: ($gutter * 3) 0;

  @include media-query($medium-up) {
    margin: ($gutter * 6) 0;
  }

  .icon-arrow-right {
    width: 7px;
    height: 7px;
    position: absolute;
    right: $gutter / 2;
    top: 50%;
    fill: $color-primary;
    @include transform(translateY(-50%));
  }
}

.password-footer {
  width: 100%;
  padding: $gutter 0;
  line-height: $gutter * 2;

  @include media-query($medium-up) {
    padding: ($gutter * 2) 0;
  }

  .icon-shopify-logo {
    width: $gutter * 6;
    height: $gutter * 2;
    margin-left: $gutter / 3;
    margin-top: -5px;
    fill: $color-footer-text;
  }
}

.password-modal {
  display: none;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background-color: $color-main-bg;
  opacity: 0;
  transition: opacity 0.15s ease-out;

  &.password-modal--open {
    display: block;
    opacity: 1;
  }
}

.password-modal__content {
  position: absolute;
  top: 50%;
  width: 100%;
  padding: 0 $gutter;
  @include transform(translateY(-50%));
}

.password-modal__close {
  position: absolute;
  right: 0;
  top: $gutter;
}


