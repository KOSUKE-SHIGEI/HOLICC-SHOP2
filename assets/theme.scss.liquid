  @include media-query($small) {
    @include flex-direction(column);
  }
}

.feature-row__item {
  @include flex(0 1 50%);

  @include media-query($small) {
    @include flex(1 1 auto);
  }
}

.feature-row__image-crop--circle,
.feature-row__image-crop--square {
