  @include media-query($medium-up) {
    margin-bottom: $gutter * 3;
  }
}

.product-recommendations__slider {
  @include display-flexbox();
  overflow-x: auto;
  padding: 0 5rem;
  -webkit-overflow-scrolling: touch;
  list-style-type: none;

  @include media-query($medium-up) {
