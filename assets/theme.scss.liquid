    margin-left: 4rem;
  }

  @include media-query($medium-up) {
    @include flex(0 0 calc(50% - 4rem));
    margin: 2rem;

    &:nth-child(n+2):last-child {
      padding-right: 0;
    }

    &:not(:first-of-type) {
      margin-left: 2rem;
    }
