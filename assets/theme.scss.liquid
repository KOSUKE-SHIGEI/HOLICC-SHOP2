  video {
    height: 100%;
  }

  // --------------------------------------------------------------
  // Fullscreen
  // --------------------------------------------------------------

  &:fullscreen .plyr__video-wrapper &:fullscreen .plyr__poster {
    background-color: #000;
  }

  /* stylelint-disable-next-line */
  &:-webkit-full-screen .plyr__video-wrapper,
  &:-webkit-full-screen .plyr__poster {
    background-color: #000;
  }

  /* stylelint-disable-next-line */
  &:-moz-full-screen .plyr__video-wrapper,
  &:-moz-full-screen .plyr__poster {
    background-color: #000;
  }

  /* stylelint-disable-next-line */
  &:-ms-fullscreen .plyr__video-wrapper,
  &:-ms-fullscreen .plyr__poster {
    background-color: #000;
  }

  // Fallback for unsupported browsers
  .plyr--fullscreen-fallback .plyr__video-wrapper,
  .plyr--fullscreen-fallback .plyr__poster {
    background-color: #000;
  }

  .plyr__control.plyr__control--overlaid {
    background-color: $settings-color-main-bg;
    border-color: rgba($settings-color-primary-heading-text, 0.05);
    border-radius: 50%;

    &.plyr__tab-focus,
    &:hover {
      color: rgba($settings-color-primary-heading-text, 0.55);
    }
  }

  .plyr__controls {
    background-color: $settings-color-main-bg;
    border-color: rgba($settings-color-primary-heading-text, 0.05);
  }

  .plyr__progress {
    input[type='range'] {
      &::-moz-range-thumb {
        box-shadow: 2px 0 0 0 $settings-color-main-bg;
      }

      &::-ms-thumb {
        box-shadow: 2px 0 0 0 $settings-color-main-bg;
      }

      &::-webkit-slider-thumb {
        box-shadow: 2px 0 0 0 $settings-color-main-bg;
      }

      &::-webkit-slider-runnable-track {
        background-image: linear-gradient(
          to right,
          currentColor var(--value, 0),
          rgba($settings-color-primary-heading-text, 0.6) var(--value, 0)
        );
      }

      &::-moz-range-track {
        background-color: rgba($settings-color-primary-heading-text, 0.6);
      }

      &::-ms-fill-upper {
        background-color: rgba($settings-color-primary-heading-text, 0.6);
      }

      &.plyr__tab-focus {
        &::-webkit-slider-runnable-track {
          box-shadow: 0 0 0 4px rgba($settings-color-primary-heading-text, 0.25);
        }

        &::-moz-range-track {
          box-shadow: 0 0 0 4px rgba($settings-color-primary-heading-text, 0.25);
        }

        &::-ms-track {
          box-shadow: 0 0 0 4px rgba($settings-color-primary-heading-text, 0.25);
        }
      }

      &:active {
        &::-moz-range-thumb {
          box-shadow: 0 0 0 3px rgba($settings-color-primary-heading-text, 0.25);
        }

        &::-ms-thumb {
          box-shadow: 0 0 0 3px rgba($settings-color-primary-heading-text, 0.25);
        }

        &::-webkit-slider-thumb {
          box-shadow: 0 0 0 3px rgba($settings-color-primary-heading-text, 0.25);
        }
      }
    }

    .plyr__tooltip {
      background-color: $settings-color-primary-heading-text;
      color: $settings-color-main-bg;

      &::before {
        border-top-color: $settings-color-primary-heading-text;
      }
    }
  }

  &.plyr--loading .plyr__progress__buffer {
    background-image: linear-gradient(
      -45deg,
      rgba($settings-color-primary-heading-text, 0.6) 25%,
      transparent 25%,
      transparent 50%,
      rgba($settings-color-primary-heading-text, 0.6) 50%,
      rgba($settings-color-primary-heading-text, 0.6) 75%,
      transparent 75%,
      transparent
    );
  }

  .plyr__volume {
    input[type='range'] {
      color: $settings-color-main-bg;

      &::-moz-range-thumb {
        box-shadow: 2px 0 0 0 $settings-color-primary-heading-text;
      }

      &::-ms-thumb {
        box-shadow: 2px 0 0 0 $settings-color-primary-heading-text;
      }

      &::-webkit-slider-thumb {
        box-shadow: 2px 0 0 0 $settings-color-primary-heading-text;
      }

      &::-webkit-slider-runnable-track {
        background-image: linear-gradient(
          to right,
          currentColor var(--value, 0),
          rgba($settings-color-main-bg, 0.6) var(--value, 0)
        );
      }

      &::-moz-range-track,
      &::-ms-fill-upper {
        background-color: rgba($settings-color-main-bg, 0.6);
      }

      &.plyr__tab-focus {
        &::-webkit-slider-runnable-track {
          box-shadow: 0 0 0 4px rgba($settings-color-main-bg, 0.25);
        }

        &::-moz-range-track {
          box-shadow: 0 0 0 4px rgba($settings-color-main-bg, 0.25);
        }

        &::-ms-track {
          box-shadow: 0 0 0 4px rgba($settings-color-main-bg, 0.25);
        }
      }

      &:active {
        &::-moz-range-thumb {
          box-shadow: 0 0 0 3px rgba($settings-color-main-bg, 0.25);
        }

        &::-ms-thumb {
          box-shadow: 0 0 0 3px rgba($settings-color-main-bg, 0.25);
        }

        &::-webkit-slider-thumb {
          box-shadow: 0 0 0 3px rgba($settings-color-main-bg, 0.25);
        }
      }
    }
  }
