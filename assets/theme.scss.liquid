/* mediar  */




.product__media-container {
  position: relative;
  width: 100%;
  margin: 0 auto;
  overflow: hidden;

  @include media-query($large-up) {
    margin-top: $gutter * 4;
  }
}

.product__media-wrapper {
  display: block;
  position: relative;
  width: 100%;
}

.product__media-preview-image {
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  cursor: pointer;
  transition: transform 0.25s ease-out;

  &:hover,
  a:focus > & {
    @include transform(scale(1.05));
  }

  .product__media-wrapper--empty & {
    cursor: default;
  }
}

.product__vendor {
  color: lighten($color-card-text, 30%);
  font-size: rem(12);
  display: block;
  letter-spacing: 1.7px;
  margin-bottom: $gutter / 2;
}

.product__content-header {
  padding: 48px 0;

  @include media-query($medium-up) {
    padding: 90px 0;
  }
}

.product__title {
  margin-bottom: 5px;
}

.product__price {
  font-size: rem(16);
  letter-spacing: 1.5px;
  margin-bottom: 0px;
}

.product__sale-price-label {
  display: none;

  .product__price--sale & {
    display: inline;
  }
}

.product__regular-price-label {
  .product__price--sale & {
    display: none;
  }
}

.product__current-price {
  .product__price--sale & {
    color: $color-primary;
    margin-right: 10px;
  }
}

.product__compare-price-label,
.product__compare-price {
  display: none;

  .product__price--sale & {
    display: inline;
  }
}

.product-price-unit {
  display: block;
  font-size: rem(12);
  color: lighten($color-body-text, 15%);
  font-weight: normal;
}

.product-price-unit--unavailable {
  display: none;
}

.product__policies {
  text-align: center;
  font-size: rem(14);
  margin-top: 2px;
}

.product__notification {
  font-weight: 700;
  margin: $gutter / 2;
/*   opacity: 0; */
  transition: all 0.35s ease-out;
}

.product__notification--success,
.product__notification--error {
  margin: 12px $gutter $gutter;
  opacity: 1;
}

.product__notification--error {
  color: $color-error;
}

.product__content {
  padding-bottom: $gutter * 4;
}

.product__content-main {
  @include display-flexbox();
  @include justify-content(space-between);
  width: 100%;

  @include media-query($small) {
    @include flex-direction(column);
  }
}

.product__description {
  @include flex-order(2);

  @include media-query($medium-up) {
    width: calc(50% - #{$gutter * 3 / 2});
  }

  @include media-query($small) {
    width: 100%;
    margin-top: $gutter * 4;
  }
}

.product__form-container {
  @include flex-order(1);

  @include media-query($small) {
    width: 100%;
  }
}

.product__form-wrapper--fixed {
  @include media-query($medium-up) {
    position: fixed;
    top: $gutter * 4;
  }
}

.product__form-wrapper--bottom {
  @include media-query($medium-up) {
    position: absolute;
    bottom: 0;
  }
}

.product-form__item {
  @include display-flexbox();
  position: relative;
  margin-bottom: $gutter / 2;
  background-color: $color-input-background;

  &:last-of-type {
    margin-bottom: $gutter * 2;
  }
}

.product-form__master-select {
  display: none;
}

.single-option-selector,
.product-form__quantity-input {
  opacity: 0;
  margin-bottom: 0;

  .supports-no-js & {
    opacity: 1;
  }
}

input[type='number'].product-form__quantity-input {
  margin-bottom: 0;
}

.single-option-selector__label,
.product-form__quantity-label {
  display: inline-block;
  position: absolute;
  padding: ($gutter / 2) $gutter;
  margin-bottom: 0;
  font-weight: $font-weight-bold;
  pointer-events: none;

  &::after {
    content: ' - ';
    display: block;
    position: absolute;
    top: 50%;
    right: 7px;
    line-height: 1px;
    font-weight: $font-weight-bold;
  }

  @include media-query($small) {
    font-size: rem(16);
  }
}

.single-option-selector__label--hidden,
.product-form__quantity-label--hidden {
  width: 15px;
  padding: 0;
  opacity: 0;
}

.single-option-selector--padding {
  padding-left: 100px;
}

.product__share-wrapper {
  text-align: center;
  padding: $gutter 0;
}

// Product Image Gallery

.product__submedia-list {
  @include display-flexbox();
  @include flex-wrap(wrap);
  @include align-items(center);
  margin-bottom: $gutter * 4;
}

.product__submedia-list--single {
  display: block;
}

.product__submedia-list-item {
  @include flex(1 1 50%);
  text-align: center;
  overflow: hidden;

  .product__submedia-list--single & {
    @include flex(none);
    width: 100%;
    max-width: 100%;
    max-height: 100%;
  }
}

.product__submedia-wrapper {
  display: block;
  position: relative;
  padding-top: 100%;

  .product__submedia-list--single & {
    padding-top: 0;
  }
}

.product__submedia-preview-image {
  position: absolute;
  top: 0;
  left: 0;
  display: block;
  width: 100%;
  height: 100%;
  cursor: pointer;
  opacity: 0;
  transition: 0.25s transform ease-out, 0.25s opacity ease-out;

  object-fit: cover;
  object-position: 50% 50%;
  font-family: 'object-fit: cover';

  > video{
    width: 100%;
  }

  &.lazyloaded {
    opacity: 1;
  }

  &:hover,
  a:focus > & {
    @include transform(scale(1.05));
  }

  .product__submedia-list--single & {
    position: relative;
  }
}

.product__thumbnail-badge {
  width: 40px;
  height: 40px;
  position: absolute;
  right: 20px;
  bottom: 20px;
  pointer-events: none;
  color: $color-heading-text;

  .icon {
    fill: $color-main-bg;
    width: 100%;
    height: 100%;
    vertical-align: baseline;
  }

  .icon-3d-badge-full-color-outline,
  .icon-video-badge-full-color-outline {
    stroke: rgba($color-heading-text, 0.05);
  }

  @include media-query($large-up) {
    width: 60px;
    height: 60px;
  }
}

@include media-query($medium-up) {
  .product__submedia-list-item {
    @include flex(1 1 33%);
  }

  .product__submedia-list--r2 {
    .product__submedia-list-item:nth-last-of-type(2),
    .product__submedia-list-item:nth-last-of-type(2)
      + .product__submedia-list-item {
      @include flex(1 1 50%);
    }
  }

  .product__submedia-list--r1 {
    .product__submedia-list-item:nth-last-of-type(4),
    .product__submedia-list-item:nth-last-of-type(4)
      ~ .product__submedia-list-item {
      @include flex(1 1 50%);
    }
  }
}

// Product Slideshow

.product-slideshow {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  height: 0;
  background-color: $color-main-bg;
  opacity: 0;
  pointer-events: none;
  z-index: $z-index-medium;
  overflow: hidden;
  transition: opacity 0.25s ease-out;
}

.product-slideshow--show {
  height: auto;
  opacity: 1;
  overflow: auto;
  pointer-events: auto;
}

.product-slideshow__close {
  position: absolute;
  right: $gutter;
  top: 5px;

  .icon {
    fill: $color-primary;
  }

  @include media-query($medium-up) {
    top: $gutter;
  }
}

.product-slideshow__content {
  height: 100%;
  padding: ($gutter * 3) 0 ($gutter * 7);

  @include media-query($medium-up) {
    padding: ($gutter * 4) 0 ($gutter * 7);
  }
}

.product-slideshow__slide {
  .product-slideshow--single & {
    cursor: default;
  }
}

.product-slideshow__controls {
  padding: $gutter $gutter 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  max-width: 320px;
  margin: 0 auto;

  @include media-query($medium-up) {
    padding: ($gutter * 2) $gutter;
  }
}

.product-slideshow__slide-select-list {
  display: flex;
  overflow: hidden;
  max-width: 144px;
}

.product-slideshow__slide-select {
  @include prefix(appearance, none, webkit moz spec);
  background-color: $color-body-text;
  border-radius: 50%;
  border-width: 0;
  margin: 0 6px;
  padding: 0;
  height: 4px;
  width: 4px;
  opacity: 0.7;
  transition: opacity 0.15s ease-out, transform 0.15s ease-out;
  flex-shrink: 0;
}

.product-slideshow__slide-select--active {
  background-color: $color-primary;
  opacity: 1;
}

.product-slideshow__arrow {
  .icon {
    width: 6px;
    margin-top: -4px;
    fill: $color-primary;
  }
}

.product-slideshow__arrow--previous {
  order: -1;
}

.product-slideshow__arrow--next {
  order: 1;
}

.product-media {
  position: relative;
  width: 100%;
  color: $settings-color-primary-heading-text;

  iframe,
  model-viewer,
  .shopify-model-viewer-ui,
  .product-media__image,
  img,
  > video,
  .plyr,
  .media-item {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    max-width: 100%;
  }

  iframe,
  model-viewer,
  .shopify-model-viewer-ui,
  .media-item,
  > video,
  .plyr,
  .plyr__video-wrapper {
    height: 100%;
  }
}

.product-media__video-wrapper {
  position: relative;
  width: 100%;
  height: 0;
  top: 50%;
  transform: translateY(-50%);
}

.product-media--video:not(.product-media--slideshow) {
  background-color: $color-video-bg;
}

.product-media--slideshow {
  display: none;
  height: 100%;

  .product-slideshow--show & {
    display: block;
  }

  &.product-media--video,
  &.product-media--external_video,
  &.product-media--model {
    width: 100%;
    margin: 0 auto;

    @include media-query($medium-up) {
      width: 80%;
    }
  }
}

.product-media--featured-product {
  padding-top: 100%;
}

.product-media__image--slideshow {
  height: 100%;
  object-fit: contain;
  font-family: 'object-fit: contain';
  pointer-events: none;

  &.lazyloaded + .sk-fading-circle {
    display: none;
  }
}

.product-media__image--featured-product {
  height: 100%;
  object-position: center center;
}

.quotes-slider {
  height: 500px;
  background-color: $settings-color-main-bg;
  position: relative;
  overflow: hidden;

  @include media-query($medium-up) {
    height: 400px;
  }
}

.quotes-slide {
  padding: 0 $gutter;
  position: absolute;
  width: 100%;
  max-width: 85%;
  top: 50%;
  left: 50%;
  opacity: 0;
  z-index: 0;
  pointer-events: auto;
  @include transform(translate(-50%, -50%));
  transition: opacity 0.5s ease-out;
}

.quotes-slide--visible {
  opacity: 1;
  z-index: $z-index-base;
}

.quote {
  margin-bottom: $gutter;
}

.quote__author {
  letter-spacing: 1px;
}

.quote__heading {
  text-align: center;
  margin-bottom: $gutter * 3;
}

.search-again-button,
.navigation__search {
  margin: ($gutter * 2) auto 0;

  @include media-query($medium-up) {
    margin: ($gutter * 3.5) auto 0;
  }
}

.supports-no-js .search-again-button {
  display: none;
}

.supports-no-js .navigation__search {
  position: relative !important; // sass-lint:disable-line no-important
  overflow: initial;
}

/*================ Section header ================*/
.section-header-image-wrapper {
  height: 278px;
  overflow: hidden;
  background-color: $color-secondary-bg;

  @include media-query($medium-up) {
    height: 390px;
  }

  @include media-query($widescreen) {
    height: 560px;
  }
}

.section-header-image {
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  height: 100%;
}

.section-header-image--parallax {
  @include media-query($medium-up) {
    background-attachment: fixed;
  }

  .is-mobile-os & {
    background-attachment: scroll;
  }
}

.section-header {
  padding: ($gutter * 2.5) 0;

  @include media-query($medium-up) {
    padding: ($gutter * 3) 0;
  }
}

.section-header--small {
  padding: ($gutter * 2) 0;

  @include media-query($medium-up) {
    padding: ($gutter * 2.5) 0;
  }
}

.section-header__title {
  margin-bottom: 0;
}
.template-cart {
  .section-header--small {
    padding: 20px 0 30px;
  }
}
.template-cart .section-header__title  {
  font-size: 20px;
  position: relative;
  width: 160px;
  margin-left:  auto;
  margin-right:  auto;
  padding: 10px;
  border-bottom: 3px solid #BEBEBE;
}

.template-cart .section-header__title:before {
  content: "";
  position: absolute;
  left: 0;
  bottom: -3px;
  width: 50px;
  height: inherit;
  border-bottom: 3px solid #43434E;
}

@media screen and (min-width: 769px){
  .section-header--small {
    padding: 40px 0;
  }
  .template-cart .section-header__title  {
    font-size: 32px;
    width: 260px;
  }
  .template-cart .section-header__title:before {
    width: 90px;
  }
}

.section-header__description,
.section-header__btn {
  margin-top: $gutter;
}

.section-header__subtext {
  display: block;

  @include media-query($medium-up) {
    margin: (-$gutter / 2) 0 ($gutter * 3.5);
  }
}

$container-padding-bottom: 35px;
$transition-buttons: all 0.35s cubic-bezier(0, 0, 0, 1);
$transition-slideshow: all 0.5s cubic-bezier(0, 0, 0, 1);
$transition-image-slideshow: opacity 0.8s cubic-bezier(0.44, 0.13, 0.48, 0.87),
  filter 400ms, -webkit-filter 400ms;
$button-size: 30px;

.slideshow {
  @include display-flexbox;
  position: relative;
  overflow: hidden;
  max-height: calc(100vh - #{$container-padding-bottom});
}

.slideshow--adapt {
  background-color: $color-blankstate-background;
}

.slideshow--full .slideshow {
  height: calc(100vh - #{$container-padding-bottom});
  @include media-query($small) {
    height: calc(85vh - #{$container-padding-bottom});
	}
}

.slideshow--adapt .slideshow {
  @include media-query($small) {
    max-height: 125vw;
    min-height: 75vw;
  }
}

.slideshow__overlay {
  @include overlay(3);
}

.slideshow__slide {
  position: absolute;
  height: 100%;
  width: 100%;
  opacity: 0;
  z-index: 0;
  transition: $transition-slideshow;
}

.slideshow__slide--active {
  transition: $transition-slideshow;
  opacity: 1;
  z-index: $z-index-base + 2;
}

.slideshow__slide--transitioning {
  opacity: 1;
  z-index: $z-index-base + 1;
}

.slideshow__image {
  height: 100%;
  width: 100%;
  object-fit: cover;
  z-index: $z-index-base + 1;
  transition: $transition-image-slideshow;
  font-family: "object-fit: cover";

  .placeholder-background {
    background-color: $color-main-bg;
  }

  .placeholder-svg {
    position: absolute;
    top: 50%;
    left: 50%;
    width: auto;
    max-height: none;
    max-width: none;
    @include transform(translate(-50%, -50%));
  }
}

.slideshow__video,
.slideshow__video iframe {
  opacity: 0;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 101%;
  visibility: hidden;
  transition: $transition-slideshow;
  pointer-events: none;

  .slideshow--adapt & {
    min-height: 56.25vw;
  }

  @include media-query($small) {
    display: none;
  }

  // https://developer.mozilla.org/en-US/docs/Web/CSS/Media_Queries/Using_media_queries#aspect-ratio
  @media (min-aspect-ratio: 16/9) {
    height: 300%;
    top: -100%;
  }

  @media (max-aspect-ratio: 16/9) {
    width: 300%;
    left: -100%;
  }

  .slideshow__video--loaded:not(.slideshow__video--paused) & {
    opacity: 1;
    visibility: visible;
  }
}

.slideshow__video::after {
  content: "";
  display: block;
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: $z-index-base;
}

.slideshow__play-button {
  opacity: 0;
  position: relative;
  margin: 0 auto;
  height: 50px;
  width: 50px;
  padding: 0;
  transition: transform 0.5s ease;

  &:hover {
    @include transform(scale(1.1));
  }

  &:hover,
  &:disabled {
    opacity: 0;
  }

  .slideshow__video--loaded & {
    opacity: 1;
  }

  @include media-query($small) {
    display: none;
  }
}

.slideshow__play-button-icon {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 8px;
  height: 8px;
  border-color: transparent transparent transparent #fff;
  border-width: 0 0 0 7px;
  border-style: double;
  transition: border 0.1s ease;
  @include transform(translate(-42%, -50%));

  .is-paused & {
    width: 0;
    height: 0;
    border-top: 4px solid transparent;
    border-left: 8px solid #fff;
    border-bottom: 4px solid transparent;
  }
}

.slideshow__play-button-circle {
  position: absolute;
  top: 50%;
  left: 50%;
  background-color: transparent;
  border: 2px solid #fff;
  height: $button-size;
  width: $button-size;
  -moz-border-radius: $button-size / 2;
  -webkit-border-radius: $button-size / 2;
  border-radius: $button-size / 2;
  @include transform(translate(-50%, -50%));
}

.slideshow__text-container {
  text-align: left;
  position: absolute;
  top: 100px;
  left: 100px;
  z-index: $z-index-base + 2;
}


.slideshow__text-content {
  opacity: 0;

  &.has-animated {
    @include prefix(animation, fadeUp 0.5s ease-out 0.5s 1 forwards, webkit spec);
  }

  .supports-no-js &,
  .animations--disabled & {
    opacity: 1;
  }

  .slideshow--adapt:not(.slideshow--center-cta) & {
    @include media-query($small) {
      display: none;
    }
  }
}

.slideshow__text-container-mobile {
  position: relative;
  transform: none;

  @include media-query($medium-up) {
    display: none;
  }
}
@media only screen and (max-width: 768px ){
  .slideshow__text-container {
      top: 80px;
      left: 20px;
  }
}

.slideshow__text-content-mobile {
  text-align: center;
  margin: 15px 0 50px;

  @include media-query($medium-up) {
    display: none;
  }
}

.slideshow__heading {
  display: none;
  line-height: 1.8;
  letter-space: 1px;
  margin: 0 auto;

  .slideshow--full &,
  .slideshow--adapt .slideshow__text-content-mobile & {
    display: block;
  }

  @include media-query($medium-up) {
    display: block;
    max-width: 800px;
  }
}

.slideshow__heading-cta {
  opacity: 0;
  margin-top: $gutter * 2;
  transition: opacity 0.6s ease-in;

  @include media-query($medium-up) {
    margin-top: $gutter * 3;
  }

  .slideshow__slide--active & {
    opacity: 1;
  }

  .slideshow--adapt.slideshow--center-cta & {
    @include media-query($small) {
      margin-top: 0;
    }
  }

  .slideshow--adapt:not(.slideshow--center-cta) & {
    @include media-query($small) {
      display: none;
    }
  }
}

.slideshow__heading-cta-text {
  min-width: 90px;
  min-height: 0px;
  display: flex;
  justify-content: center;
  opacity: 0;
  transition: min-height 0.20s ease-out, min-width 0.20s ease-out, opacity 0.15s ease-out;
}

.slideshow__heading-cta-text--active {
  opacity: 1;
}

.slideshow__button-cta-single {
  display: inline-block;
  margin-top: 0;
  position: absolute;
  transform: translateX(-50%) translateY(calc(-50% - 15px));

  @include media-query($medium-up) {
    display: none;
  }
}

.slideshow__buttons {
  @include display-flexbox;
  align-items: flex-end;
  position: absolute;
  bottom: 0;
  z-index: $z-index-base + 4;
  transition: $transition-buttons;
  overflow: visible;
  transform-style: flat;

  .supports-no-js & {
    display: none;
  }
}

.slideshow__button {
  @include display-flexbox;
  @include flex-direction(column);
  @include align-items(center);
  @include flex(0 0 50%);
  position: relative;
  text-align: center;
  white-space: initial;
  transition: $transition-buttons;

  @include media-query($small) {
    @include transform(translateX(50%));
    margin-bottom: $gutter;
  }

  @include media-query($medium-up) {
    @include flex(1 1 100%);

    &:hover:not(.slideshow__button--active) {
      @include transform(translate3d(0, -16px, 0));
    }
  }

  &:not(.slideshow__button--active) {
    @include align-self(flex-end);
  }
}

.slideshow__button--4,
.slideshow__button--5 {
  @include media-query($medium-up) {
    @include transform(translateX(-50%));

    &.slideshow__button--active {
      @include transform(translate3d(-50%, -($gutter * 3), 0));
    }

    .slideshow--center-cta &.slideshow__button--active,
    &:hover:not(.slideshow__button--active) {
      @include transform(translate3d(-50%, -16px, 0));
    }
  }
}

.slideshow__button--4 {
  @include media-query($medium-up) {
    @include flex(1 0 33%);
  }
}

.slideshow__button--5 {
  @include media-query($medium-up) {
    @include flex(1 0 25%);
  }
}

.slideshow__button--active {
  @include transform(translate3d(50%, -($gutter * 3), 0));

  &:not(.slideshow__button--link) {
    @include transform(translate3d(50%, -16px, 0));

    @include media-query($medium-up) {
      @include transform(translate3d(0, -16px, 0));

      &.slideshow__button--4,
      &.slideshow__button--5 {
        @include transform(translate3d(-50%, -16px, 0));
      }
    }
  }

  .slideshow--center-cta & {
    @include transform(translate3d(50%, -16px, 0));

    @include media-query($medium-up) {
      @include transform(translate3d(0, -16px, 0));

      &.slideshow__button--4,
      &.slideshow__button--5 {
        @include transform(translate3d(-50%, -16px, 0));
      }
    }
  }

  @include media-query($medium-up) {
    @include transform(translate3d(0, -($gutter * 3), 0));
  }
}

.slideshow__navigation-item {
  position: absolute;
  height: 100%;
  width: 10vh;
  background: transparent;
  border: 0;
  z-index: $z-index-base + 6;

  @include media-query($medium-up) {
    visibility: hidden;
    z-index: $z-index-base + 4;
  }

  &.slideshow__navigation-item--no-focus {
    outline: none;
  }
}

.slideshow__button-next {
  right: 0;
}

.slideshow__button-label {
  color: $color-heading-text;

  @include media-query($medium-up) {
    padding: $gutter ($gutter * 2);
    margin: 4px 0 $container-padding-bottom 0;
  }
}

.slideshow__button-label-text {
  line-height: rem(30);

  .slideshow--center-cta .slideshow__button--active &,
  .slideshow--center-cta .slideshow__button:hover & {
    border-bottom: 2px solid;
    padding-bottom: 5px;
  }
}

.slideshow__button-cta {
  position: absolute;
  left: 50%;
  bottom: -62px;
  @include transform(translateX(-50%));

  @include media-query($medium-up) {
    bottom: -30px;
  }

  .slideshow__button:not(.slideshow__button--active) & {
    pointer-events: none;
  }

  .slideshow__button--active.slideshow__button--link & {
    bottom: -50px;
  }

  .slideshow__button--active:not(.slideshow__button--link) & {
    bottom: (-$gutter) * 4;

    @include media-query($medium-up) {
      bottom: (-$gutter) * 3;
    }
  }
}

.slideshow__button-cta--multiple {
  padding: 0;
  height: 40px;
  width: 40px;
  line-height: 40px;
  border: 0;

  .icon-plus {
    height: 8px;
    width: 8px;
    fill: $color-button-text;
  }
}

.slideshow__indicators {
  @include display-flexbox;
  @include align-items(center);
  @include justify-content(center);
  z-index: $z-index-base + 4;

  .supports-no-js & {
    display: none;
  }

  @include media-query($small) {
    height: 35px;
  }

  @include media-query($medium-up) {
    display: none;
  }
}

.slideshow__indicator {
  display: block;
  background-color: $color-body-text;
  border-radius: 6px;
  border-width: 0;
  margin: 0 5px;
  padding: 0;
  height: 4px;
  width: 4px;
  opacity: 0.7;
  transition: opacity 0.15s ease-out;
}

.slideshow__indicator--active {
  background-color: $color-primary;
  opacity: 1;
}

@include keyframes(fadeUp) {
  0% {
    @include transform(translate3d(0, 10%, 0));
    opacity: 0;
  }

  100% {
    @include transform(none);
    opacity: 1;
  }
}

$site-header-wrapper-color: rgba($color-header, 0);

/*================ Site Header ================*/

.site-header {
  position: relative;
  transition: transform 0.25s ease-out;
  z-index: $z-index-header;
}

.site-header--transparent {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  z-index: $z-index-header;

  html.supports-no-js & {
    position: relative;
  }
}

.site-header--fixed {
  background-color: #fff;
  position: fixed;
  top: 0;
  width: 100%;
  @include prefix(animation, slideDown 0.25s ease-out, webkit spec);
}

.site-header__wrapper {
  position: relative;
  width: 100%;
  background-color: $color-header;
  z-index: $z-index-header;
  transition: background-color 0.25s ease-out;
  -webkit-transform:translate3d(0,0,0);
  .site-header--transparent & {
    background-color: $site-header-wrapper-color;
  }

  .navigation-open &,
  html.supports-no-js & {
    background-color: $color-header;
  }

  .site-header--fixed & {
    background-color: $color-header;
  }

  svg {
    fill: $color-header-links;
  }
}

.site-header__wrapper--transitioning {
  transition: background-color 0.25s ease-out, margin-top 0.25s ease-out;
}

.announcement-bar {
  display: none;
  text-align: center;
  position: relative;
  text-decoration: none;
  z-index: $z-index-header;
}

.announcement-bar--visible {
  display: block;
}

.announcement-bar__message {
  font-size: em(16);
  font-weight: $font-weight-header;
  padding: 10px $gutter;
  margin-bottom: 0;

  @include media-query($small) {
    padding: 10px calc(#{$gutter} + 30px);
  }

  .icon {
    fill: currentColor;
  }
}

.announcement-bar__close {
  position: absolute;
  right: $gutter;
  padding: 0;
  width: 30px;
  height: 30px;
  line-height: 100%;
  top: 50%;
  @include transform(translateY(-50%));

  @include media-query($small) {
    right: 10px;
  }

  .icon {
    width: 12px;
    height: 12px;
    fill: currentColor;
  }
}

.site-header-sections {
  @include display-flexbox;
  @include align-items(center);
}

.site-header__section--title {
  @include display-flexbox;
  @include justify-content(center);
}

.site-header__logo-wrapper {
  display: inline-block;
  margin-bottom: 0;
  padding: ($gutter / 2) 0;
  text-align: center;
  line-height: 1.2;
}

.site-header__logo {
  display: block;
}

.site-header__logo-image {
  display: block;
  margin: 0 auto;

  .site-header--transparent & {
    display: none;
  }

  .navigation-open &,
  html.supports-no-js & {
    display: block;
  }

  .site-header--fixed & {
    display: block;
  }
}

.site-header__logo-image--transparent {
  display: none;

  .site-header--transparent & {
    display: block;
  }

  .navigation-open &,
  html.supports-no-js & {
    display: none;
  }

  .site-header--fixed & {
    display: none;
  }
}

.site-header__cart {
  svg {
    width: 22px;
    height: 30px;
    margin: -2px 0 0 -2px;

    @include media-query($medium-up) {
      width: 26px;
    }
  }

  &::after {
    border-color: $color-header-links;
  }
}

.site-header__cart-bubble {
  position: absolute;
  top: 15px;
  right: 10px;
  width: 10px;
  height: 10px;
  background-color: $color-cart-flag;
  border-radius: 50%;
  opacity: 0;
  transition: all 0.25s ease-out;
  @include transform(scale(0));

  @include media-query($medium-up) {
    top: 15px;
    right: 9px;
  }

  .site-header__cart & {
    top: 13px;

    @include media-query($medium-up) {
      top: 12px;
    }
  }
}

.site-header__cart-bubble--visible {
  opacity: 1;
  @include transform(scale(1));

  &.bubble-pulse {
    @include prefix(animation, bubble-pulse 0.5s ease-out, webkit spec);
  }
}

@include keyframes(bubble-pulse) {
  0% {
    @include transform(scale(1));
  }

  25% {
    @include transform(scale(1.4));
  }

  50% {
    @include transform(scale(1));
  }

  75% {
    @include transform(scale(1.4));
  }

  100% {
    @include transform(scale(1));
  }
}

.burger-icon {
  display: block;
  height: 2px;
  width: 18px;
  left: 50%;
  margin-left: -9px;
  background: $color-header-links;
  position: absolute;
  transition: all 0.25s ease-out;

  .navigation-open & {
    width: 24px;
    top: calc(50% - 1px);
    left: calc(50% - 4px);
  }

  @include media-query($medium-up) {
    height: 2px;
    width: 24px;
    left: 50%;
    margin-left: -12px;

    .navigation-open & {
      width: 30px;
      top: calc(50% - 1px);
      left: calc(50% - 2px);
    }
  }
}

.burger-icon--top {
  top: 17px;

  .navigation-open & {
    @include transform(rotate(45deg));
  }

  @include media-query($medium-up) {
    top: 16px;

    .site-header__navigation:hover & {
      @include transform(translateY(-2px));

      .navigation-open & {
        width: 35px;
        left: calc(50% - 4px);
        @include transform(rotate(45deg));
      }
    }
  }
}

.burger-icon--mid {
  top: 24px;

  @include media-query($medium-up) {
    top: 24px;
  }

  .navigation-open & {
    opacity: 0;
  }
}

.burger-icon--bottom {
  top: 31px;

  .navigation-open & {
    @include transform(rotate(-45deg));
  }

  @include media-query($medium-up) {
    top: 32px;

    .site-header__navigation:hover & {
      @include transform(translateY(2px));

      .navigation-open & {
        width: 35px;
        left: calc(50% - 4px);
        @include transform(rotate(-45deg));
      }
    }
  }
}

.site-header__navigation {
  position: relative;
  transition: transform 0.15s ease-out;

  &:hover {
    opacity: 1;
  }

  svg {
    width: 20px;
    height: 30px;
  }

  &::after {
    border-color: $color-header-links;
  }
}

.social-links {
  .icon {
    fill: $color-footer-text;
  }

  a {
    margin-right: $gutter;
    text-decoration: none;
    opacity: 1;
    transition: opacity 0.15s ease-out;
  }

  a:hover,
  a:focus {
    opacity: $link-hover-opacity;
  }

  a:last-child {
    margin-right: 0;
  }
}

.social-links--large {
  .icon {
    width: 24px;
    height: 24px;
    margin-top: 6px;
  }
}

.social-links__icon {
  margin-right: $gutter / 2;

  &:last-child {
    margin-right: 0;
  }
}

$social-sharing-link-border-color: rgba($color-body-text, 0.1);

/*================ Social Sharing ================*/

.social-sharing {
  position: relative;
  display: inline-block;
  margin: 0 auto;
}

.social-sharing--product {
  margin-bottom: 80px;
}

.social-sharing--article {
  &.social-sharing--1 {
    height: 145px;
  }

  &.social-sharing--2 {
    height: 215px;
  }

  &.social-sharing--3 {
    height: 285px;
  }
}

.social-sharing__toggle {
  padding: $gutter;
  z-index: $z-index-base;
  cursor: pointer;

  .social-sharing--article & {
    padding: 0;
  }
}

.social-sharing__item-list {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 0;
  padding: 0;
  margin: 0;
  list-style: none;

  .social-sharing--mobile & {
    box-sizing: content-box;
    max-height: 50px;
  }
}

.social-sharing__item {
  display: inline-block;
  position: absolute;
  top: 0;
  left: 50%;
  width: 50px;
  height: 50px;
  margin: 0 0 0 -25px;
  opacity: 0;
  pointer-events: none;
  transition: all 0.15s ease-out;

  .social-sharing--active & {
    opacity: 1;
    pointer-events: auto;
  }
}

.social-sharing__link {
  display: inline-block;
  width: 50px;
  height: 50px;
  line-height: 45px;
  border-radius: 50%;
  text-align: center;
  border: solid 2px $social-sharing-link-border-color;
  transition: all 0.15s ease-out;

  &:hover {
    border: solid 2px $social-sharing-link-border-color;
  }

  pointer-events: none;
  background-color: $color-main-bg;

  .social-sharing--active & {
    opacity: 1;
    pointer-events: auto;
  }

  .icon {
    fill: $color-body-text;
  }
}

.social-sharing__icon {
  .social-sharing--product & {
    margin-right: 10px;
  }

  .social-sharing--mobile & {
    display: block;
    width: 50px;
    height: 50px;
    padding: 8px 0;
    margin: 0 auto ($gutter / 2);
    text-align: center;
    border: 2px solid $color-primary;
    border-radius: 50px;
  }

  .social-sharing--article & {
    display: block;
    width: 100%;
    height: 15px;
    padding: 0;
    margin-bottom: 15px;
    text-align: center;

    .icon {
      vertical-align: top;
    }
  }

  .icon {
    fill: $color-primary;
    height: 15px;
  }

  .icon-share {
    display: inline-block;
    transition: all 0.15s ease-out;
    @include transform(rotateX(0deg));

    .social-sharing--active & {
      opacity: 0;
      @include transform(rotateX(180deg));
    }
  }

  .icon-close {
    display: inline-block;
    margin-left: -25px;
    opacity: 0;
    transition: all 0.15s ease-out;
    @include transform(rotateX(0deg));

    .social-sharing--active & {
      opacity: 1;
      @include transform(rotateX(180deg));
    }
  }

  .icon-facebook {
    height: 22px;
  }
}

.social-sharing__item:nth-of-type(1) {
  .social-sharing--1.social-sharing--mobile.social-sharing--active & {
    @include transform(translate(0, -60px));
  }

  .social-sharing--1.social-sharing--product.social-sharing--active & {
    @include transform(translate(0, 65px));
  }

  .social-sharing--2.social-sharing--mobile.social-sharing--active & {
    @include transform(translate(-30px, -60px));
  }

  .social-sharing--2.social-sharing--product.social-sharing--active & {
    @include transform(translate(-30px, 65px));
  }

  .social-sharing--3.social-sharing--mobile.social-sharing--active & {
    @include transform(translate(-60px, -45px));
  }

  .social-sharing--3.social-sharing--product.social-sharing--active & {
    @include transform(translate(-65px, 65px));
  }

  .social-sharing--article.social-sharing--active & {
    @include transform(translate(0, 105px));
  }
}

.social-sharing__item:nth-of-type(2) {
  transition-delay: 0.05s;
  -webkit-transition-delay: 0.05s;

  .social-sharing--active & {
    opacity: 1;
  }

  .social-sharing--2.social-sharing--mobile.social-sharing--active & {
    @include transform(translate(30px, -60px));
  }

  .social-sharing--2.social-sharing--product.social-sharing--active & {
    @include transform(translate(30px, 65px));
  }

  .social-sharing--3.social-sharing--mobile.social-sharing--active & {
    @include transform(translate(0, -60px));
  }

  .social-sharing--3.social-sharing--product.social-sharing--active & {
    @include transform(translate(0, 65px));
  }

  .social-sharing--article.social-sharing--active & {
    @include transform(translate(0, 165px));
  }
}

.social-sharing__item:nth-of-type(3) {
  transition-delay: 0.1s;
  -webkit-transition-delay: 0.1s;

  .social-sharing--active & {
    opacity: 1;
  }

  .social-sharing--3.social-sharing--mobile.social-sharing--active & {
    @include transform(translate(60px, -45px));
  }

  .social-sharing--3.social-sharing--product.social-sharing--active & {
    @include transform(translate(65px, 65px));
  }

  .social-sharing--article.social-sharing--active & {
    @include transform(translate(0, 225px));
  }
}

/*================ Slider ================*/
.slider {
  position: relative;
  width: 100%;
  height: 100%;
}

.slider__slide {
  display: none;
  position: absolute;
  width: 100%;
  height: 100%;
  cursor: pointer;
  cursor: -webkit-grab;
  cursor: -moz-grab;
}

.slider__slide--active {
  display: block;
  @include transform(translateX(0));
}

.slider__slide--previous {
  display: block;
  @include transform(translateX(-100%));

}

.slider__slide--next {
  display: block;
  @include transform(translateX(100%));
}

.slider__slide--transitioning {
  transition: transform 0.2s ease-out;
}

.slider__slide--dragging {
  cursor: -webkit-grabbing;
  cursor: -moz-grabbing;
}

.sk-fading-circle {
  position: absolute;
  top: 50%;
  left: 50%;
  margin: -20px 0 0 -10px;
  width: 40px;
  height: 40px;
}

.supports-no-js .sk-fading-circle {
  display: none;
}

.sk-fading-circle--top {
  margin-top: $gutter * 2.5;
}

.sk-circle {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}

.sk-circle::before {
  content: '';
  display: block;
  margin: 0 auto;
  width: 15%;
  height: 15%;
  background-color: $color-primary;
  border-radius: 100%;
  @include prefix(animation, sk-circleFadeDelay 1.2s infinite ease-in-out both, webkit spec);
}

@for $i from 1 through 11 {
  $item: $i + 1;
  $deg: ($i * 30) + deg;
  $time: -(1.2 - ($i * 0.1)) + s;

  .sk-circle#{$item}::before {
    @include prefix(animation-delay, $time, webkit spec);
  }

  .sk-circle#{$item} {
    @include prefix(transform, rotate($deg), webkit ms spec);
  }
}

@include keyframes(sk-circleFadeDelay) {
  0%,
  39%,
  100% {
    opacity: 0;
  }

  40% {
    opacity: 1;
  }
}

@include keyframes(sk-circleFadeDelay) {
  0%,
  39%,
  100% {
    opacity: 0;
  }

  40% {
    opacity: 1;
  }
}

$video-section-primary-color: #fff;
$video-section-icon-color: #4a4a4a;
$video-section-overlay-background-color: rgba(0, 0, 0, 0.8);
$video-section-button-background-color: rgba(0, 0, 0, 0);

.video-section {
  position: relative;
  height: 56.25vw;
  min-height: 300px;
  max-height: 810px;
  background-size: cover;
  background-color: $color-secondary-bg;

  &.lazyloaded {
    background-color: transparent;
  }
}

.video-section__body-overlay {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 0;
  background-color: $video-section-overlay-background-color;
  opacity: 0;
  pointer-events: none;
  transition: opacity 0.25s ease-out;

  .video-section--loaded & {
    height: 100%;
    opacity: 1;
    pointer-events: auto;
    z-index: $z-index-high;
  }
}

.video-section__player,
.video-section__player iframe {
  position: absolute;
  width: 100%;
  height: 100%;

  .video-section--loaded & {
    z-index: $z-index-high + 1;
  }
}

.video-section__player-close {
  display: none;
  position: absolute;
  left: $gutter;
  top: $gutter;
  background-color: $video-section-primary-color;
  border-radius: 50%;

  .video-section--loaded & {
    z-index: $z-index-high + 2;
  }

  .icon {
    fill: $video-section-icon-color;
    width: 15px;
    height: 15px;
  }

  @include media-query($small) {
    top: $gutter / 2;
    left: $gutter / 2;
  }
}

.video-section__cover {
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  z-index: $z-index-base;
  opacity: 1;
  transition: all 0.25s ease-out;

  &.video-section--loaded {
    display: none;
    opacity: 0;
  }
}

.video-section__cover-background {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-size: cover;
  opacity: 1;
  transition: opacity 0.25s ease-out;
}

.video-section__cover-overlay {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0.1;
}

.video-section__cover-content {
  position: absolute;
  top: 50%;
  width: 100%;
  padding: 0 $gutter;
  text-align: center;
  opacity: 1;
  transition: opacity 0.25s ease-out;
  @include transform(translateY(-50%));
}

.video-section__cover-text {
  color: $video-section-primary-color;
  text-align: center;
  margin: 0 auto ($gutter * 2);
  max-width: 750px;

  @include media-query($small) {
    font-size: rem(24);
    margin-bottom: $gutter;
  }
}

.video-section__error {
  display: none;
  position: absolute;
  bottom: -40px;
  width: 50vw;
  min-width: 340px;
  left: 50%;
  color: $video-section-primary-color;
  @include transform(translateX(-50%));

  .video-section--error & {
    display: block;
  }
}

.video-section__load-player-button {
  position: relative;
  margin: 0 auto;
  height: 70px;
  width: 70px;
  padding: 0;
  transition: all 0.5s ease;

  &:hover {
    opacity: 1;
    @include transform(scale(1.1));
  }

  &.animated.pulse {
    outline: none;
    @include prefix(animation-name, pulse, ms webkit spec);
    @include prefix(animation-duration, 1s, ms webkit spec);
    @include prefix(animation-fill-mode, both, ms webkit moz spec);
    @include prefix(animation-iteration-count, infinite, ms webkit moz spec);
  }

  @include media-query($small) {
    height: 50px;
    width: 50px;
  }
}

.video-section__load-player-button-triangle {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 0;
  height: 0;
  border-top: 8px solid transparent;
  border-left: 15px solid $video-section-primary-color;
  border-bottom: 8px solid transparent;
  @include transform(translate(-42%, -50%));
}

.video-section__load-player-button-circle {
  position: absolute;
  top: 50%;
  left: 50%;
  background-color: $video-section-button-background-color;
  border: 2px solid $video-section-primary-color;
  height: 70px;
  width: 70px;
  -moz-border-radius: 75px;
  -webkit-border-radius: 75px;
  border-top-right-radius: 75px; //Expanded border radius for IE9 & 10
  border-top-left-radius: 75px;
  border-bottom-right-radius: 75px;
  border-bottom-left-radius: 75px;
  @include transform(translate(-50%, -50%));

  @include media-query($small) {
    height: 50px;
    width: 50px;
  }

  .video-section__load-player-button.animated & {
    position: absolute;
    top: 0;
    left: 0;
    border: 3px dotted $video-section-primary-color;
    @include prefix(animation-name, spin, ms webkit moz spec);
    @include prefix(animation-duration, 4s, ms webkit moz spec);
    @include prefix(animation-iteration-count, infinite, ms webkit moz spec);
    @include prefix(animation-timing-function, linear, ms webkit moz spec);
    @include prefix(animation-fill-mode, both, ms webkit moz spec);
  }
}

@include keyframes(spin) {
  from {
    @include prefix(transform, rotate(0deg), ms webkit moz spec);
  }

  to {
    @include prefix(transform, rotate(360deg), ms webkit moz spec);
  }
}

@include keyframes(pulse) {
  0% {
    @include prefix(transform, scale(1), webkit spec);
  }

  50% {
    @include prefix(transform, scale(1.1), webkit spec);
  }

  100% {
    @include prefix(transform, scale(1), webkit spec);
  }
}

$featured-slider-primary-color: #fff;
$featured-slider-close-background-color: rgba(0, 0, 0, 0);

/*================ General ================*/
.featured-slider {
  position: relative;
  overflow: hidden;
  background-color: $color-secondary-bg;
}

/*================ Cover ================*/
.featured-slider__cover-background {
  position: absolute;
  left: 0;
  width: 50%;
  height: 100vh;
  overflow: hidden;
  z-index: $z-index-base;

  &:after {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 101%;
  }

  .placeholder-svg {
    width: 150vw;
    height: 50vw;
    max-width: none;
    max-height: none;
    transform-origin: 0 0;
    @include transform(rotate(90deg) translate(-10%, -100%));
  }

  @include media-query($small) {
    display: block;
    width: 100%;
    height: 50vh;
    transition: transform 0.25s ease-out, opacity 0.25s ease-out;

    .placeholder-svg {
      width: 300vw;
      height: 100vw;
    }
  }
}

.featured-slider__cover-background--top {
  top: 0;

  @include media-query($medium-up) {
    .side-scroller--fixed & {
      position: fixed;
      will-change: transform;
    }

    .side-scroller--fixed-bottom & {
      top: auto;
      bottom: 0;
    }
  }

  @include media-query($small) {
    .open & {
      @include transform(translateY(-100%));
    }

    .placeholder-svg {
      position: absolute;
      bottom: 0;
      @include transform(rotate(90deg) translate(-16.7%, -100%));
    }
  }
}

.featured-slider__cover-background--bottom {
  display: none;
  bottom: 0;
  height: 50%;

  @include media-query($small) {
    display: block;

    .open & {
      transition: transform 0.25s ease-out, opacity 0.25s ease-out;
      @include transform(translateY(100%));
    }

    .placeholder-svg {
      position: absolute;
      top: 0;
      left: 0;
      @include transform(rotate(90deg) translate(-50%, -100%));
    }
  }
}

.featured-slider__cover-background-image {
  position: absolute;
  left: 0;
  width: 100%;
  height: 100%;
  background-size: cover;
  background-position: center center;
  background-color: $color-secondary-bg;

  @include media-query($small) {
    height: 200%;

    .featured-slider__cover-background--top & {
      top: 0;
    }

    .featured-slider__cover-background--bottom & {
      bottom: 0;
    }
  }
}

.featured-slider__cover-content {
  position: absolute;
  top: 50vh;
  left: 50%;
  width: 33%;
  opacity: 1;
  transition: opacity 0.25s ease-out;
  z-index: $z-index-medium;
  text-align: center;
  @include transform(translate(-50%, -50%));

  @include media-query($medium-up) {
    .side-scroller--fixed & {
      position: fixed;
      will-change: transform;
    }

    .side-scroller--fixed-bottom & {
      top: auto;
      bottom: 50vh;
      @include transform(translate(-50%, 50%));
    }
  }

  @include media-query($small) {
    width: 80%;

    .open & {
      opacity: 0;
      pointer-events: none;
    }
  }
}

.featured-slider__cover-content--left {
  @include media-query($medium-up) {
    left: 25%;
  }
}

.featured-slider__cover-title {
  padding-bottom: 4px;
  cursor: default;
  opacity: 0;
  @include transform(translateY(20%));

  &.has-animated {
    @include prefix(animation, fadeInUp 0.5s ease-out 0.25s 1 forwards, webkit spec);
  }

  .supports-no-js &,
  .animations--disabled & {
    opacity: 1;
  }

  @include media-query($small) {
    border-bottom: 2px solid $featured-slider-primary-color;
    cursor: pointer;
  }
}

.pinch-indicator {
  position: absolute;
  left: 50%;
  width: 25px;
  height: 25px;
  background-color: $featured-slider-primary-color;
  opacity: 0.66;
  border-radius: 20px;
  pointer-events: none;
  @include transform(translate(-50%));

  @include prefix(animation-duration, 2s, ms webkit moz spec);
  @include prefix(animation-iteration-count, infinite, ms webkit moz spec);
  @include prefix(animation-timing-function, ease-out, ms webkit moz spec);
  @include prefix(animation-fill-mode, both, ms webkit moz spec);

  @include media-query($medium-up) {
    display: none;
  }
}

.pinch-indicator--top {
  top: -40px;
  @include prefix(animation-name, pinch-up, ms webkit moz spec);
}

.pinch-indicator--bottom {
  bottom: -40px;
  @include prefix(animation-name, pinch-down, ms webkit moz spec);
}

@include keyframes(pinch-up) {
  0%,
  15% {
    opacity: 0;
  }

  15%,
  30% {
    @include transform(translate(-50%, 0));
    opacity: 0.66;
  }

  70%,
  100% {
    @include transform(translate(-50%, -100px));
    opacity: 0;
  }
}

@include keyframes(pinch-down) {
  0%,
  15% {
    opacity: 0;
  }

  15%,
  30% {
    @include transform(translate(-50%, 0%));
    opacity: 0.66;
  }

  70%,
  100% {
    @include transform(translate(-50%, 100px));
    opacity: 0;
  }
}

/*================ Close ================*/
.featured-slider__close {
  display: none;
  position: absolute;
  top: 2 * $gutter;
  left: 50%;
  height: 50px;
  width: 50px;
  padding: 0;
  transition: all 0.5s ease;
  z-index: $z-index-high;
  @include transform(scale(1) translateX(-50%));

  &:hover {
    opacity: 1;
    @include transform(scale(1.1) translateX(-50%));
  }

  @include media-query($small) {
    .featured-slider.open & {
      display: block;
    }
  }
}

.featured-slider__close-icon {
  position: absolute;
  top: 50%;
  left: 50%;
  @include transform(translate(-50%, -50%));

  .icon {
    fill: $featured-slider-primary-color;
  }
}

.featured-slider__close-circle {
  position: absolute;
  top: 50%;
  left: 50%;
  background-color: $featured-slider-close-background-color;
  border: 2px solid $featured-slider-primary-color;
  height: 70px;
  width: 70px;
  -moz-border-radius: 75px;
  -webkit-border-radius: 75px;
  border-top-right-radius: 75px; // Expanded border radius for IE9 & 10
  border-top-left-radius: 75px;
  border-bottom-right-radius: 75px;
  border-bottom-left-radius: 75px;
  @include transform(translate(-50%, -50%));

  @include media-query($small) {
    height: 50px;
    width: 50px;
  }
}

/*================ Slides ================*/
.featured-slider__slides-container {
  transition: background-color 1s ease-out;

  @include media-query($small) {
    height: 100vh;
    overflow: hidden;
  }

  @include media-query($medium-up) {
    min-height: 100vh;
    width: 50%;
    margin-left: 50%;
  }
}

.featured-slider__slide-list {
  @include media-query($medium-up) {
    padding: 20vh 0;
  }
}

.featured-slider__slide {
  position: relative;
  text-align: center;

  @include media-query($medium-up) {
    display: block;
    cursor: default;
  }

  @include media-query($small) {
    height: 100vh;
    display: none;
    position: absolute;

    &.slider__slide--active,
    &.slider__slide--previous,
    &.slider__slide--next {
      display: block;
    }
  }
}

.featured-slider__slide-content {
  position: relative;
  margin: 0 auto;
  max-width: 45%;

  @include media-query($medium-up) {
    padding: 20vh 0;
  }

  @include media-query($small) {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 100%;
    max-width: 65vw;
    @include transform(translate(-50%, -45%));
  }
}

.featured-slider__slide-image-container {
  position: relative;
  pointer-events: none;
}

.featured-slider__slide-image {
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  pointer-events: none;

  &.placeholder-svg {
    position: relative;
    max-width: 450px;
    margin: 0 auto;
    background-color: transparent;
    transform: scale(1.5);
  }
}

.featured-slider__slide-title {
  margin: ($gutter * 2) 0 $gutter;
  font-weight: $font-weight-semi-bold;
  letter-spacing: 1px;
  text-transform: uppercase;
}

/*================ Mobile Reveal Slider ================*/

.reveal-slider__nav {
  display: none;
  position: absolute;
  bottom: $gutter;
  left: 50%;
  @include transform(translateX(-50%));

  @include media-query($small) {
    display: block;
  }
}

.reveal-slider__nav-button {
  @include prefix(appearance, none, webkit moz spec);
  border-radius: 6px;
  border-width: 0;
  margin: 0 4px;
  padding: 0;
  height: 4px;
  width: 4px;
  opacity: 0.7;
  transition: opacity 0.15s ease-out;

  &.active {
    opacity: 1;
  }
}

.reveal-slider__previous,
.reveal-slider__next {
  position: absolute;
  top: 0;
  bottom: 0;
  width: 45px;
  opacity: 0;

  @include media-query($medium-up) {
    display: none;
  }
}

.reveal-slider__previous {
  left: 0;
}

.reveal-slider__next {
  right: 0;
}

.reveal-slider__previous-button,
.reveal-slider__next-button {
  display: inline-block;
  width: 100%;
  height: 100%;
}

.map-section {
  position: relative;
  height: 650px;
  width: 100%;
  overflow: hidden;

  @include media-query($medium-up) {
    height: 500px;
  }

  .page-width {
    height: 100%;
  }
}

.map-section__overlay {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0.1;
}

.map-section__error-message {
  display: none;
  position: absolute;
  top: 50%;
  left: 50%;
  max-width: 400px;
  padding: $gutter;
  color: $color-error;
  border: 3px solid $color-error;
  background-color: lighten($color-error, 35%);
  font-weight: $font-weight-bold;
  @include transform(translateY(-50%));

  @include media-query($small) {
    @include transform(translate(-50%, 0));
    margin-top: $gutter;
    text-align: center;
  }
}

.map-section__content-wrapper {
  position: relative;
  text-align: center;
  height: 100%;
}

.map-section__content {
  position: relative;
  display: inline-block;
  background-color: $color-main-bg;
  padding: $gutter * 2;
  margin-top: $gutter;
  width: 100%;
  text-align: center;
  z-index: $z-index-base;

  @include media-query($medium-up) {
    position: absolute;
    left: 0;
    top: 50%;
    @include transform(translateY(-50%));
    margin-top: 0;
    width: percentage(1 / 3);
  }
}

.map-section__link {
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: $z-index-base - 1;
}

// Optically center map in visible area with
// extended height/widths and negative margins
.map-section__container {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 150%;
  margin-bottom: -50%;

  @include media-query($medium-up) {
    width: 130%;
    height: 100%;
    margin: 0 -30% 0 0;
  }
}

.map-section__button {
  display: inline-block;
  margin-top: $gutter;
  white-space: normal;
}

// Hide Google maps UI
.gm-style-cc,
.gm-style-cc + div {
  visibility: hidden;
}

.highlight {
  padding: ($section-mobile-space + ($gutter * 3)) 0 $section-mobile-space;

  @include media-query($medium-up) {
    padding: $section-desktop-space 0;
  }
}

.highlight__wrapper {
  position: relative;
  padding: ($gutter * 3) ($gutter * 2);

  @include media-query($medium-up) {
    @include display-flexbox();
  }
}

.highlight__content-container,
.highlight__image-container {
  @include media-query($medium-up) {
    @include flex(1);
    width: 50%;

    .highlight--align-top & {
      @include align-self(flex-start);
    }

    .highlight--align-middle & {
      @include align-self(center);
    }

    .highlight--align-bottom & {
      @include align-self(flex-end);
    }
  }
}

.highlight__content-container {
  @include media-query($medium-up) {
    @include flex-order(2);
  }

  @include media-query($small) {
    width: 100%;
    margin-top: $gutter * 3;
  }
}

.highlight__border {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  pointer-events: none;
  z-index: $z-index-base;

  &.has-animated,
  .supports-no-js &,
  .animations--disabled & {
    .highlight__line--top,
    .highlight__line--bottom {
      transition: width 0.5s ease-out 0.75s;
      width: 100%;
    }

    .highlight__line--right,
    .highlight__line--left {
      transition: height 0.5s ease-out 0.75s;
      height: 100%;
    }
  }
}

.highlight__line {
  position: absolute;
}

.highlight__line--top {
  top: 0;
  left: 0;
  height: 3px;
  width: 0;
}

.highlight__line--right {
  top: 0;
  right: 0;
  height: 0;
  width: 3px;
}

.highlight__line--bottom {
  bottom: 0;
  right: 0;
  height: 3px;
  width: 0;
}

.highlight__line--left {
  bottom: 0;
  left: 0;
  height: 0;
  width: 3px;
}

.highlight__image-container {
  position: relative;

  @include media-query($medium-up) {
    @include flex-order(1);
  }

  @include media-query($small) {
    position: relative;
    width: 100%;
    margin-top: -($gutter * 5);
    z-index: $z-index-base + 1;
  }
}

.highlight__image-container--front {
  @include media-query($medium-up) {
    z-index: $z-index-base + 2;
  }
}

.highlight__image-container--back {
  @include media-query($medium-up) {
    z-index: 0;
  }
}

.highlight__image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;

  @include media-query($small) {
    display: block;
    margin: 0 auto;
  }

  @include media-query($medium-up) {
    margin-left: -25%;
  }
}

.highlight__image--placeholder {
  position: relative;
}

$slow-drawer-transition: transform 0.45s cubic-bezier(0.645, 0.045, 0.355, 1);
$fast-drawer-transition: transform 0.55s cubic-bezier(0.645, 0.045, 0.355, 1);

.drawer {
  position: fixed;
  top: 0;
  right: -$cart-mobile-width;
  width: $cart-mobile-width;
  height: 100vh;
  transition: $fast-drawer-transition;
  @include transform(translateX(0));
  z-index: $z-index-high;

  @include media-query($medium-up) {
    right: -$cart-desktop-width;
    width: $cart-desktop-width;
  }

  .drawer--active & {
    display: block;
    transition: $slow-drawer-transition;
    @include transform(translateX(-$cart-mobile-width));

    @include media-query($medium-up) {
      @include transform(translateX(-$cart-desktop-width));
    }
  }
}

[data-drawer-push] {
  transition: $slow-drawer-transition;

  .drawer--active & {
    transition: $fast-drawer-transition;
    @include transform(translateX(-$cart-mobile-width));

    @include media-query($medium-up) {
      @include transform(translateX(-$cart-desktop-width));
    }
  }
}

.drawer-cover {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 0;
  background-color: $color-drawer-overlay;
  opacity: 0;
  pointer-events: none !important; // sass-lint:disable-line no-important
  z-index: $z-index-high;
  transition: opacity 0.25s ease-out;

  .drawer--active & {
    opacity: 0.3;
    height: 100%;
    pointer-events: auto !important; // sass-lint:disable-line no-important
  }
}

.ajax-cart__template {
  display: none;
}

.ajax-cart__discount-container--active {
  max-height: 100px;
  opacity: 1;
}


